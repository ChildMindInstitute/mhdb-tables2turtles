	mhdb:hasPossibleContrast mhdb:stop_trial ;
	mhdb:hasImplementation mhdb:Python_stop_signal_task ;
	mhdb:hasImplementation mhdb:STOP-IT ;
	mhdb:hasImplementation mhdb:Cognition_Lab-SST ;
	mhdb:hasIndicator mhdb:accuracy ;
